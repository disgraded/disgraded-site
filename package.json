{
  "name": "disgraded-site",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "run-p sass:build sass:watch react:start",
    "build": "run-p sass:build react:build",
    "deploy": "run-s build deploy:github",
    "test": "react-scripts test",
    "react:build": "react-scripts build",
    "react:start": "react-scripts start",
    "deploy:github": "gh-pages -b master -d build -message deploy-0.0.2",
    "sass:build": "node-sass --output-style compressed style/index.scss public/style.css",
    "sass:watch": "node-sass -w --output-style compressed style/index.scss public/style.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "enum": "^2.5.0",
    "history": "^4.9.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-tap-event-plugin": "^3.0.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5"
  }
}
